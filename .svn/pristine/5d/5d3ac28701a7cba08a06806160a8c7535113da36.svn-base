#countdown_holder {
	height: 30px;
}
#countdown_dashboard {
	height: 30px;
	width: 216px;
	float:right;
}

.dash {
	width: 39px;
	height: 36px;
	float: left;
	margin: 0px;
	padding: 0px;
	position: relative;
	color: #333;
	background-color: #004b5e;
	border-right: 1px solid white;
	background-image: none;
}

