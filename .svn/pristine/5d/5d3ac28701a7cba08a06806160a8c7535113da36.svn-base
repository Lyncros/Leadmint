.dash .digit {
	font: bold 20px arial;
	color: white;
	font-weight: bold;
	float: left;
	width: 0px;
	line-height: 15px;
	text-align: center;
	position: relative;
	margin-right: 0px;
	text-shadow: black 1px 1px;
	margin-left: 11px;
}

.dash_title {
	display: block;
	color: white;
	font-family: arial;
	font-size: 9px;
	margin-left: 13px;
	text-shadow: black 1px 1px;
}
